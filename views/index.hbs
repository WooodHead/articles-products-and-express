
{{{productMessages}}}
{{#each products.productList}}
  <div class="products">
    <h1>Name: {{name}}</h1>
    <h2>Price: {{price}}</h2>
    <h3>Inventory: {{inventory}}</h3>
  </div>
{{/each}}

{{#each articles.articleList}}
  <div class="articles">
    <h1>Title: {{title}}</h1>
    <h2>Body: {{body}}</h2>
    <h3>Author: {{author}}</h3>
  </div>
{{/each}}